<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rightsallocate.model.Function" table="function">
		<id name="fid" type="java.lang.String">
			<column name="fid" length="36" />
			<generator class="uuid" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="100" />
		</property>
		<property name="des" type="java.lang.String">
			<column name="des" length="100" />
		</property>
		<set name="resourceList">
          <key column="fid"></key>
          <one-to-many class="com.rightsallocate.model.Resource"/>
        </set>
		
		<set name="roleList" table="role_function">
        	<key column="functionid"></key>
        	<many-to-many class="com.rightsallocate.model.Role" column="roleid"></many-to-many>
        </set>
	</class>

</hibernate-mapping>
