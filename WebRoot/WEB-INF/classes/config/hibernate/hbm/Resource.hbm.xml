<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.rightsallocate.model.Resource" table="resource">
		<id name="rid" type="java.lang.String">
			<column name="rid" length="36" />
			<generator class="uuid" />
		</id>
		<property name="name" type="java.lang.String">
			<column name="name" length="100" />
		</property>
		<property name="value" type="java.lang.String">
			<column name="value" length="100" />
		</property>
		<property name="des" type="java.lang.String">
			<column name="des" length="100" />
		</property>
		<many-to-one name="function" column="fid">
             
        </many-to-one>
		
		<set name="roleList" table="role_resource">
        	<key column="resourceid"></key>
        	<many-to-many class="com.rightsallocate.model.Role" column="roleid"></many-to-many>
        </set>
	</class>

</hibernate-mapping>
